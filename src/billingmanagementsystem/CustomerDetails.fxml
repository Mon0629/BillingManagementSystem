<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="384.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="billingmanagementsystem.CustomerDetailsController">
	<children>
		<HBox alignment="BOTTOM_CENTER" prefHeight="70.0" prefWidth="445.0">
			<children>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Customer Details" textAlignment="CENTER" wrappingWidth="444.13671875">
					<font>
						<Font name="HP Simplified Light" size="36.0" />
					</font>
					<HBox.margin>
						<Insets />
					</HBox.margin>
				</Text>
			</children>
		</HBox>
		<HBox alignment="CENTER_LEFT" prefHeight="70.0" prefWidth="600.0">
			<children>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="First Name" wrappingWidth="79.0" />
				<TextField fx:id="firstNameFid" alignment="BASELINE_LEFT" prefHeight="26.0" prefWidth="200.0" />
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Last Name" textAlignment="CENTER" wrappingWidth="90.0" />
				<TextField fx:id="lastNameFid" alignment="BASELINE_LEFT" prefHeight="25.0" prefWidth="200.0" />
			</children>
		</HBox>
		<HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="68.0" prefHeight="70.0" prefWidth="445.0">
			<children>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Email" wrappingWidth="57.13671875" />
				<TextField fx:id="emailFid" alignment="BASELINE_LEFT" prefHeight="26.0" prefWidth="281.0" />
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Contact No." textAlignment="CENTER" wrappingWidth="87.13668823242188" />
				<TextField fx:id="contactFid" alignment="BASELINE_LEFT" prefHeight="26.0" prefWidth="154.0" />
			</children>
		</HBox>
		<HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="98.0" prefHeight="70.0" prefWidth="445.0">
			<children>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Address" wrappingWidth="70.0" />
            <TextField fx:id="addressFid" alignment="BASELINE_LEFT" prefHeight="25.0" prefWidth="500.0" />
			</children>
         <padding>
            <Insets bottom="5.0" top="5.0" />
         </padding>
		</HBox>
		<HBox alignment="CENTER_LEFT" prefHeight="70.0" prefWidth="445.0">
			<children>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Town" wrappingWidth="70.0" />
				<TextField fx:id="townFid" alignment="BASELINE_LEFT" prefHeight="25.0" prefWidth="500.0" />
			</children>
		</HBox>
		<HBox alignment="CENTER_LEFT" prefHeight="70.0" prefWidth="445.0">
			<children>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Country" wrappingWidth="70.0" />
				<TextField fx:id="countryFid" alignment="BASELINE_LEFT" prefHeight="25.0" prefWidth="250.0" />
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Postal" textAlignment="CENTER" wrappingWidth="70.0" />
				<TextField fx:id="postalFid" alignment="BASELINE_LEFT" prefHeight="25.0" prefWidth="100.0" />
			</children>
		</HBox>
		<HBox alignment="CENTER" prefHeight="70.0" prefWidth="600.0">
			<children>
				<Button fx:id="saveButton" mnemonicParsing="false" onMouseClicked="#save" style="-fx-background-color: rgb(54, 48, 98); -fx-border-radius: 10;" text="Save">
					<HBox.margin>
						<Insets right="10.0" />
					</HBox.margin>
				</Button>
				<Button fx:id="clearButton" mnemonicParsing="false" onMouseClicked="#clear" style="-fx-background-color: rgb(54, 48, 98); -fx-border-radius: 500;" text="Clear">
					<HBox.margin>
						<Insets left="10.0" />
					</HBox.margin>
				</Button>
			</children>
		</HBox>
	</children>
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
</VBox>
